
<section class="staff">
  <h2>Instructors</h2>
  <ul>
    {{#each staff}}
    <li>
      <p>{{this.first_name}} {{this.last_name}}</p>
      <a href="/staff/delete/{{this.id}}"><i class="fa fa-remove"></i></a>
    </li>
    {{/each}}
  </ul>
  <a href="/staff/new"><button type="button">Add Staff</button></a>

</section>
